          "concentration": false
        },
        "materials": {
          "value": "a drop of water and a pinch of dust",
          "consumed": false,
          "cost": 0,
          "supply": 0
        },
        "target": {
          "value": null,
          "units": "",
          "type": ""
        },
        "actionType": "save",
        "save": {
          "ability": "str",
          "dc": null
