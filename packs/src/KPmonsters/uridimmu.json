      "type": "spell",
      "img": "icons/mystery-man.png",
      "name": "Glyph Of Warding",
      "data": {
        "source": "",
        "prepared": true,
        "level": 3,
        "school": "abj",
        "activation": {
          "type": "hour",
          "cost": "1",
          "condition": ""
        },
        "range": {
          "value": 0,
          "units": "touch"
        },
        "duration": {
          "value": null,
          "units": "Until dispelled or triggered"
        },
        "components": {
          "value": "",
          "vocal": true,
          "somatic": true,
          "material": true,
          "ritual": false,
          "concentration": false
        },
        "materials": {
          "value": "incense and powdered diamond worth at least 200 gp, which the spell consumes",
          "consumed": false,
          "cost": 200,
          "supply": 0
        },
        "target": {
          "value": null,
          "units": "",
          "type": ""
