      "type": "spell",
      "img": "systems/dnd5e/icons/spells/runes-magenta-3.jpg",
      "name": "Teleport",
      "data": {
        "source": "",
        "prepared": true,
        "level": 7,
        "school": "con",
        "activation": {
          "type": "action",
          "cost": "1",
