      "type": "spell",
      "img": "systems/dnd5e/icons/spells/rock-orange-3.jpg",
      "name": "Earthquake",
      "data": {
        "source": "",
        "prepared": true,
        "level": 8,
        "school": "evo",
        "activation": {
          "type": "action",
          "cost": "1",
          "condition": ""
