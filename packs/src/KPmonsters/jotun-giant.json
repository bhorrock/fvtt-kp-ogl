        "components": {
          "value": "",
          "vocal": true,
          "somatic": true,
          "material": true,
          "ritual": false,
          "concentration": false
        },
        "materials": {
          "value": "a pinch of dirt, a piece of rock, and a lump of clay",
          "consumed": false,
          "cost": 0,
          "supply": 0
        },
        "target": {
          "value": 100,
          "units": "ft",
          "type": "radius"
        },
        "actionType": "save",
        "save": {
          "ability": "con",
          "dc": null
